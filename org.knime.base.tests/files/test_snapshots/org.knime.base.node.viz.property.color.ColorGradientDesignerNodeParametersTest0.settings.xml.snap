<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="http://www.knime.org/2008/09/XMLConfig" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.knime.org/2008/09/XMLConfig http://www.knime.org/XMLConfig_2008_09.xsd" key="test">
    <config key="columnFilter">
        <entry key="mode" type="xstring" value="MANUAL"/>
        <config key="patternFilter">
            <entry key="pattern" type="xstring" value=""/>
            <entry key="isCaseSensitive" type="xboolean" value="false"/>
            <entry key="isInverted" type="xboolean" value="false"/>
        </config>
        <config key="manualFilter">
            <config key="manuallySelected">
                <entry key="array-size" type="xint" value="3"/>
                <entry key="0" type="xstring" value="Index0"/>
                <entry key="1" type="xstring" value="Index2"/>
                <entry key="2" type="xstring" value="Index4"/>
            </config>
            <config key="manuallyDeselected">
                <entry key="array-size" type="xint" value="2"/>
                <entry key="0" type="xstring" value="Index1"/>
                <entry key="1" type="xstring" value="Index3"/>
            </config>
            <entry key="includeUnknownColumns" type="xboolean" value="false"/>
        </config>
        <config key="typeFilter">
            <config key="selectedTypes">
                <entry key="array-size" type="xint" value="0"/>
            </config>
        </config>
    </config>
    <entry key="gradient" type="xstring" value="CUSTOM"/>
    <entry key="valueScale" type="xstring" value="PERCENTAGE"/>
    <config key="customGradient">
        <config key="0">
            <entry key="stopValue" type="xdouble" value="0.0"/>
            <entry key="color" type="xstring" value="#FF0000"/>
        </config>
        <config key="1">
            <entry key="stopValue" type="xdouble" value="50.0"/>
            <entry key="color" type="xstring" value="#00FF00"/>
        </config>
        <config key="2">
            <entry key="stopValue" type="xdouble" value="100.0"/>
            <entry key="color" type="xstring" value="#0000FF"/>
        </config>
    </config>
    <entry key="missingValueColor" type="xstring" value="#FFCCFF"/>
    <entry key="nanColor" type="xstring" value="#FF66FF"/>
    <entry key="negativeInfinityColor" type="xstring" value="#FFFF00"/>
    <entry key="belowMinColor" type="xstring" value="#00FFFF"/>
    <entry key="aboveMaxColor" type="xstring" value="#00FFFF"/>
    <entry key="positiveInfinityColor" type="xstring" value="#FFFF00"/>
</config>
