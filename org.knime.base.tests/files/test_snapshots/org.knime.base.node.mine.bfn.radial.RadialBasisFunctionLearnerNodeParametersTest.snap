  "schema" : {
    "type" : "object",
    "properties" : {
      "model" : {
        "type" : "object",
        "properties" : {
          "distance" : {
            "type" : "integer",
            "format" : "int32",
            "default" : 0
          },
          "maxClassCoverage" : {
            "type" : "boolean",
            "title" : "Use class with max coverage",
            "description" : "If selected, only the class with maximum coverage degree of the target columns is used during\ntraining, otherwise all class columns are considered for coverage.\n",
            "default" : true
          },
          "maxEpochsParameters" : {
            "type" : "object",
            "properties" : {
              "maxEpochs" : {
                "type" : "integer",
                "format" : "int32",
                "title" : "Maximum number of epochs",
                "description" : "The maximum number of epochs the algorithm has to process the entire data set.\n",
                "default" : 42
              },
              "useMaxEpochs" : {
                "type" : "boolean",
                "title" : "Use maximum number of epochs",
                "description" : "If selected, the option defines the maximum number of epochs the algorithm has to process the\nentire data set, otherwise it repeats this process until this rule model is stable, i.e. no new\nrule has been committed and/or no shrink is executed.\n",
                "default" : false
              }
            },
            "default" : {
              "useMaxEpochs" : false,
              "maxEpochs" : 42
            }
          },
          "missing" : {
            "type" : "string",
            "title" : "Missing values",
            "description" : "Select one method to handle missing values <br/>\n<ul>\n    <li>\n        <b>Incorp</b>\n        May generate fuzzy rules with missing values, if no replacement value has been found during the\n        learning process.\n    </li>\n    <li>\n        <b>Best Guess</b>\n        Computes the optimal replacement value by projecting the fuzzy rule (with missing value(s)) onto the\n        missing dimension(s) of all other rules.\n    </li>\n    <li>\n        <b>Mean</b>\n        Replaces the missing value(s) with the column mean.\n    </li>\n    <li>\n        <b>Min</b>\n        Replaces the missing value(s) with the column min.\n    </li>\n    <li>\n        <b>Max</b>\n        Replaces the missing value(s) with the column max.\n    </li>\n    <li>\n        <b>Zero</b>\n        Replaces the missing value(s) with zero.\n    </li>\n    <li>\n        <b>One</b>\n        Replaces the missing value(s) with one.\n    </li>\n</ul>\n"
          },
          "shrinkAfterCommit" : {
            "type" : "boolean",
            "title" : "Shrink after commit",
            "description" : "If selected, a shrink to reduce conflicting rules is executed\nimmediately after a new rule is committed, i.e. the new rule is reduced so that conflicts with\nall other rules of different classes are avoided.\n",
            "default" : false
          },
          "targetColumns" : {
            "type" : "array",
            "items" : {
              "type" : "string"
            },
            "title" : "Target columns",
            "description" : "Select the target(s) to be used for classification. If more than one column (only numeric) is\nselected, the columns must contain class membership values between 0 and 1 for each class given\nby the column name.\n",
            "default" : [ ]
          },
          "thetaMinus" : {
            "type" : "number",
            "format" : "double",
            "title" : "Theta minus",
            "description" : "This defines the upper boundary of activation for conflicting rules.\n",
            "default" : 0.2
          },
          "thetaPlus" : {
            "type" : "number",
            "format" : "double",
            "title" : "Theta plus",
            "description" : "This defines the lower boundary of activation for non-conflicting rules.\n",
            "default" : 0.4
          }
        }
      }
    }
  },
