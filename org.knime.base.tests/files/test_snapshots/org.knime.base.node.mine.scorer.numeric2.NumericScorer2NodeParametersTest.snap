          "flowVariablePrefix" : {
            "type" : [ "string", "null" ],
            "title" : "Export and prefix flow variables",
            "description" : "If enabled, the scores will be exported as flow variables with hard coded names and prefixed by the given\nvalue.\n",
            "default" : ""
          },
          "numberOfPredictors" : {
            "type" : "integer",
            "format" : "int32",
            "title" : "Number of predictors",
            "description" : "The number of predictors used to compute the adjusted R squared. The adjusted R squared is calculated as:\n1 - ((1 - RÂ²) * (n - 1)) / (n - p - 1), where n is the number of observations and p is the number of\npredictors.\n",
            "default" : 0
          },
          "outputColumnName" : {
            "type" : "string",
            "title" : "Output column name",
            "description" : "The name of the column in the output.\n"
          },
          "overrideOutput" : {
            "type" : "boolean",
            "title" : "Override output column name",
            "description" : "Overrides the name of the column in the output. By default, the name of the predicted column is used.\n",
            "default" : false
          },
          "predictedColumn" : {
            "type" : "string",
            "title" : "Predicted column",
            "description" : "Column with the modeled, predicted data values.\n"
          },
          "referenceColumn" : {
            "type" : "string",
            "title" : "Reference column",
            "description" : "Column with the correct, observed, training data values.\n"
          }
        }
      }
    }
  },
