<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.knime.filehandling.core.MountPointFileSystemAccess" name="MountPointFileSystemAccess" schema="schema/org.knime.filehandling.core.MountPointFileSystemAccess.exsd"/>
   <extension-point id="org.knime.filehandling.core.FSConnectionProvider" name="FSConnectionProvider" schema="schema/org.knime.filehandling.core.FSConnectionProvider.exsd"/>
   <extension-point id="org.knime.filehandling.core.FSDescriptorProvider" name="FSDescriptorProvider" schema="schema/FSDescriptorProvider.exsd"/>
   
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/cfilehandling16x16.png"
            level-id="file-handling-prototype"
            locked="true"
            name="File Handling (Labs)"
            path="/labs/">
      </category>
      <category
            after="write"
            icon="icons/file_system_connector.png"
            level-id="connectors"
            locked="true"
            name="Connectors"
            path="/io/">
      </category>
   </extension>
   <extension
         point="org.knime.core.PortType">
      <portType
            color="#178BA5"
            hidden="false"
            name="File System"
            objectClass="org.knime.filehandling.core.port.FileSystemPortObject"
            objectSerializer="org.knime.filehandling.core.port.FileSystemPortObject$Serializer"
            specClass="org.knime.filehandling.core.port.FileSystemPortObjectSpec"
            specSerializer="org.knime.filehandling.core.port.FileSystemPortObjectSpec$Serializer">
      </portType>
   </extension>
   <extension
         point="org.knime.core.DataType">
      <DataType
            cellClass="org.knime.filehandling.core.data.location.cell.FSLocationCell">
         <serializer
               cellClass="org.knime.filehandling.core.data.location.cell.FSLocationCell"
               serializerClass="org.knime.filehandling.core.data.location.cell.FSLocationCell$PathSerializer">
         </serializer>
      </DataType>
      <DataType
            cellClass="org.knime.filehandling.core.data.location.cell.SimpleFSLocationCell"
            factoryClass="org.knime.filehandling.core.data.location.cell.SimpleFSLocationCell$Factory">
         <serializer
               cellClass="org.knime.filehandling.core.data.location.cell.SimpleFSLocationCell"
               serializerClass="org.knime.filehandling.core.data.location.cell.SimpleFSLocationCell$SimpleFSLocationCellSerializer">
         </serializer>
      </DataType>
   </extension>
   <extension
         point="org.knime.core.DataColumnMetaDataType">
      <DataColumnMetaData
            dataColumnMetaDataExtension="org.knime.filehandling.core.data.location.FSLocationValueMetaDataExtension">
      </DataColumnMetaData>
   </extension>
   <extension
         point="org.knime.core.DataValueRenderer">
      <dataValue
            valueClass="org.knime.filehandling.core.data.location.FSLocationValue">
         <renderer
               rendererFactoryClass="org.knime.filehandling.core.data.location.FSLocationValueRenderer$PathFactory"
               suggestAsDefault="true">
         </renderer>
         <renderer
               rendererFactoryClass="org.knime.filehandling.core.data.location.FSLocationValueRenderer$CompleteLocationFactory"
               suggestAsDefault="false">
         </renderer>
      </dataValue>
   </extension>
   <extension
         point="org.knime.core.FlowVariableType">
      <VariableType
            extension="org.knime.filehandling.core.data.location.variable.FSLocationVariableType$FSLocationVariableTypeExtension">
      </VariableType>
      <VariableType
            extension="org.knime.filehandling.core.data.location.variable.FSLocationSpecVariableType$FSLocationSpecVariableTypeExtension">
      </VariableType>
   </extension>
   <extension
         point="org.knime.filehandling.core.FSDescriptorProvider">
      <FSDescriptorProvider
            class="org.knime.filehandling.core.connections.local.LocalFSDescriptorProvider">
      </FSDescriptorProvider>
      <FSDescriptorProvider
            class="org.knime.filehandling.core.connections.knimeremote.KNIMERemoteFSDescriptorProvider">
      </FSDescriptorProvider>
      <FSDescriptorProvider
            class="org.knime.filehandling.core.connections.url.URIFSDescriptorProvider">
      </FSDescriptorProvider>
   </extension>
</plugin>
